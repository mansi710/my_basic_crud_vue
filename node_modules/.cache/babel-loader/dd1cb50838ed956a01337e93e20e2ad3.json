{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"EditBlog\",\n\n  data() {\n    return {\n      blog: {\n        title: \"\",\n        description: \"\",\n        status: \"\",\n        tag: \"\",\n        order: \"\",\n        _method: \"patch\"\n      }\n    };\n  },\n\n  mounted() {\n    this.showBlog();\n  },\n\n  methods: {\n    showBlog() {\n      axios.get(`/editBlog/${this.$route.params.id}`).then(response => {\n        const {\n          title,\n          description,\n          status\n        } = response.data.blog;\n        this.blog.title = title;\n        this.blog.description = description;\n        this.blog.status = status;\n        this.blog.tag = tag;\n        this.blog.order = order;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    updateBlog() {\n      axios.put(`/updateBlog/${this.$route.params.id}`, this.blog).then(response => console.log(response)).catch(error => console.log(error));\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwFA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,EAAE,UADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE,EADH;QAEJC,WAAW,EAAE,EAFT;QAGJC,MAAM,EAAE,EAHJ;QAIJC,GAAG,EAAE,EAJD;QAKJC,KAAK,EAAE,EALH;QAMJC,OAAO,EAAE;MANL;IADD,CAAP;EAUD,CAbY;;EAcbC,OAAO,GAAG;IACR,KAAKC,QAAL;EACD,CAhBY;;EAiBbC,OAAO,EAAE;IACPD,QAAQ,GAAG;MACTX,KAAI,CACDa,GADH,CACQ,aAAY,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAAG,EAD1C,EAEGC,IAFH,CAESC,QAAD,IAAc;QAClB,MAAM;UAAEd,KAAF;UAASC,WAAT;UAAsBC;QAAtB,IAAmCY,QAAQ,CAAChB,IAAT,CAAcC,IAAvD;QACA,KAAKA,IAAL,CAAUC,KAAV,GAAkBA,KAAlB;QACA,KAAKD,IAAL,CAAUE,WAAV,GAAwBA,WAAxB;QACA,KAAKF,IAAL,CAAUG,MAAV,GAAmBA,MAAnB;QACA,KAAKH,IAAL,CAAUI,GAAV,GAAgBA,GAAhB;QACA,KAAKJ,IAAL,CAAUK,KAAV,GAAkBA,KAAlB;MACD,CATH,EAUGW,KAVH,CAUUC,KAAD,IAAW;QAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACD,CAZH;IAaD,CAfM;;IAgBPG,UAAU,GAAG;MACXvB,KAAI,CACDwB,GADH,CACQ,eAAc,KAAKV,MAAL,CAAYC,MAAZ,CAAmBC,EAAG,EAD5C,EAC+C,KAAKb,IADpD,EAEGc,IAFH,CAESC,QAAD,IAAcG,OAAO,CAACC,GAAR,CAAYJ,QAAZ,CAF