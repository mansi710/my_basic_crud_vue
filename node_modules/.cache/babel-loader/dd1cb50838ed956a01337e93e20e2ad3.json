   </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"tag\" class=\"col-sm-2 col-form-label\">Enter Tag</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"tag\"\n          placeholder=\"enter your tag\"\n          v-model=\"blog.tag\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"order\" class=\"col-sm-2 col-form-label\">Enter order</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"number\"\n          class=\"form-control\"\n          id=\"order\"\n          placeholder=\"enter your order\"\n          v-model=\"blog.order\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <div class=\"col-sm-10\">\n        <button type=\"submit\" class=\"btn btn-primary\">Save</button>\n      </div>\n    </div>\n  </form>\n</template>\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"EditBlog\",\n  data() {\n    return {\n      blog: {\n        title: \"\",\n        description: \"\",\n        status: \"\",\n        tag: \"\",\n        order: \"\",\n        _method: \"patch\",\n      },\n    };\n  },\n  mounted() {\n    this.showBlog();\n  },\n  methods: {\n    showBlog() {\n      axios\n        .get(`/editBlog/${this.$route.params.id}`)\n        .then((response) => {\n          const { title, description, status,  } = response.data.blog;\n          this.blog.title = title;\n          this.blog.description = description;\n          this.blog.status = status;\n          this.blog.tag = tag;\n          this.blog.order = order;\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    updateBlog() {\n      axios\n        .put(`/updateBlog/${this.$route.params.id}`, this.blog)\n        .then((response) => console.log(response))\n        .catch((error) => console.log(error));\n    },\n  },\n};\n</script>\n<style>\n#page {\n  margin-top: 100px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}