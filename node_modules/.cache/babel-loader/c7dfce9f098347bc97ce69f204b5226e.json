{"ast":null,"code":"import axios from \"axios\";\nimport ValidationError from \"@/components/ValidationError.vue\";\nexport default {\n  name: \"CreatePost\",\n\n  data() {\n    return {\n      components: {\n        ValidationError\n      },\n      validationErrors: \"\",\n      login: {\n        // userId: \"\",\n        email: \"\",\n        password: \"\"\n      }\n    };\n  },\n\n  methods: {\n    CreatePost() {\n      axios.post(\"login\", this.login).then(response => {\n        localStorage.setItem(\"token\", response.data.access_token);\n\n        if (response.status == 200) {\n          alert(\"sucess\");\n          this.$router.push(\"/home\");\n        }\n      }).catch(error => {\n        if (error.response.status == 401) {\n          // alert(\"credentials are wrong\");\n          // console.log((this.validationErrors = error.response.data.error));\n          this.validationErrors = error.response.data.wrongCredentials;\n        }\n\n        if (error.response.status == 422) {\n          this.validationErrors = error.response.data.errors; // this.validationErrors = error.response.data.emptyData;\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6GA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,eAAP,MAA4B,kCAA5B;AACA,eAAe;EACbC,IAAI,EAAE,YADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,UAAU,EAAE;QAAEH;MAAF,CADP;MAELI,gBAAgB,EAAE,EAFb;MAGLC,KAAK,EAAE;QACL;QACAC,KAAK,EAAE,EAFF;QAGLC,QAAQ,EAAE;MAHL;IAHF,CAAP;EASD,CAZY;;EAabC,OAAO,EAAE;IACPC,UAAU,GAAG;MACXV,KAAI,CACDW,IADH,CACQ,OADR,EACiB,KAAKL,KADtB,EAEGM,IAFH,CAESC,QAAD,IAAc;QAClBC,YAAY,CAACC,OAAb,CAAqB,OAArB,EAA8BF,QAAQ,CAACV,IAAT,CAAca,YAA5C;;QACA,IAAIH,QAAQ,CAACI,MAAT,IAAmB,GAAvB,EAA4B;UAC1BC,KAAK,CAAC,QAAD,CAAL;UACA,KAAKC,OAAL,CAAaC,IAAb,CAAkB,OAAlB;QACF;MACD,CARH,EASGC,KATH,CASUC,KAAD,IAAW;QAChB,IAAIA,KAAK,CAACT,QAAN,CAAeI,MAAf,IAAyB,GAA7B,EAAkC;UAChC;UACA;UACA,KAAKZ,gBAAL,GAAwBiB,KAAK,CAACT,QAAN,CAAeV,IAAf,CAAoBoB,gBAA5C;QACF;;QACA,IAAID,KAAK,CAACT,QAAN,CAAeI,MAAf,IAAyB,GAA7B,EAAkC;UAChC,KAAKZ,gBAAL,GAAwBiB,KAAK,CAACT,QAAN,CAAeV,IAAf,CAAoBqB,MAA5C,CADgC,CAEhC;QACF;MACD,CAnBH;IAoBD;;EAtBM;AAbI,CAAf","names":["axios","ValidationError","name","data","components","validationErrors","login","email","password","methods","CreatePost","post","then","response","localStorage","setItem","access_token","status","alert","$router","push","catch","error","wrongCredentials","errors"],"sourceRoot":"","sources":["/var/www/html/newData/mansi/my-vuejs-final/src/views/LoginC.vue"],"sourcesContent":["<template>\n  <link\n    href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css\"\n    rel=\"stylesheet\"\n    integrity=\"sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC\"\n    crossorigin=\"anonymous\"\n  />\n  <!-- <div class=\"container-fluid\" id=\"page\">\n    <div class=\"row\">\n      <div class=\"col-3\"></div>\n      <div class=\"col-6\">\n        <div class=\"card\">\n          <h3 class=\"card-header\">Login</h3>\n          <div class=\"card-body\">\n            <validation-errors\n              :errors=\"validationErrors\"\n              v-if=\"validationErrors\"\n            >\n              {{ validationErrors }}\n            </validation-errors>\n            <form>\n              <div class=\"form-group\">\n                <input\n                  type=\"text\"\n                  name=\"email\"\n                  v-model=\"login.email\"\n                  placeholder=\"email@example.com\"\n                  class=\"form-control\"\n                />\n              </div>\n              <div class=\"form-group\">\n                <input\n                  type=\"text\"\n                  name=\"password\"\n                  v-model=\"login.password\"\n                  placeholder=\"****\"\n                  class=\"form-control\"\n                />\n              </div>\n              <div class=\"form-group\">\n                <center>\n                  <button\n                    class=\"btn-success btn\"\n                    type=\"button\"\n                    @click=\"CreatePost\"\n                  >\n                    Submit\n                  </button>\n                </center>\n              </div>\n            </form>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div> -->\n  <form @submit.prevent=\"CreatePost\" id=\"page\">\n    <div class=\"card-header\">\n      <h4>Login</h4>\n    </div>\n    <div>\n      <validation-errors\n        :errors=\"validationErrors\"\n        class=\"alert alert-danger\"\n        v-if=\"validationErrors\"\n        id=\"err\"\n      >\n        {{ validationErrors }}\n      </validation-errors>\n    </div>\n\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"title\" class=\"col-sm-2 col-form-label\"> Enter Email </label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"title\"\n          placeholder=\"enter your email\"\n          v-model=\"login.email\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"title\" class=\"col-sm-2 col-form-label\">Enter Password </label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"title\"\n          placeholder=\"enter your password\"\n          v-model=\"login.password\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <div class=\"col-sm-10\">\n        <button type=\"submit\" class=\"btn btn-primary\">Login</button>\n      </div>\n    </div>\n    <br />\n    <router-link to=\"/apiForgotPassword\">Forgot Password</router-link>\n  </form>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport ValidationError from \"@/components/ValidationError.vue\";\nexport default {\n  name: \"CreatePost\",\n  data() {\n    return {\n      components: { ValidationError },\n      validationErrors: \"\",\n      login: {\n        // userId: \"\",\n        email: \"\",\n        password: \"\",\n      },\n    };\n  },\n  methods: {\n    CreatePost() {\n      axios\n        .post(\"login\", this.login)\n        .then((response) => {\n          localStorage.setItem(\"token\", response.data.access_token);\n          if (response.status == 200) {\n            alert(\"sucess\");\n            this.$router.push(\"/home\");\n          }\n        })\n        .catch((error) => {\n          if (error.response.status == 401) {\n            // alert(\"credentials are wrong\");\n            // console.log((this.validationErrors = error.response.data.error));\n            this.validationErrors = error.response.data.wrongCredentials;\n          }\n          if (error.response.status == 422) {\n            this.validationErrors = error.response.data.errors;\n            // this.validationErrors = error.response.data.emptyData;\n          }\n        });\n    },\n  },\n};\n</script>\n<style scoped>\n#err {\n  margin-top: 1000px;\n}\n#page {\n  margin-top: 100px;\n  padding-top: px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}