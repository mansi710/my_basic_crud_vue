{"ast":null,"code":"export default {\n  data() {\n    return {\n      current_page\n    };\n  },\n\n  props: {\n    pagination: {\n      type: Object,\n      required: true\n    },\n    offset: {\n      type: Number,\n      default: 4\n    }\n  },\n  computed: {\n    pagesNumber() {\n      if (!this.pagination.to) {\n        return [];\n      }\n\n      let from = this.pagination.current_page - this.offset;\n\n      if (from < 1) {\n        from = 1;\n      }\n\n      let to = from + this.offset * 2;\n\n      if (to >= this.pagination.last_page) {\n        to = this.pagination.last_page;\n      }\n\n      let pagesArray = [];\n\n      for (let page = from; page <= to; page++) {\n        pagesArray.push(page);\n      }\n\n      return pagesArray;\n    }\n\n  },\n  methods: {\n    changePage(page) {\n      console.log(this.pagination.current_page = page);\n      this.$emit(\"paginate\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgCA,eAAe;EACXA,IAAI,GAAE;IACF,OAAO;MACHC;IADG,CAAP;EAGH,CALU;;EAMbC,KAAK,EAAE;IACLC,UAAU,EAAE;MACVC,IAAI,EAAEC,MADI;MAEVC,QAAQ,EAAE;IAFA,CADP;IAKLC,MAAM,EAAE;MACNH,IAAI,EAAEI,MADA;MAENC,OAAO,EAAE;IAFH;EALH,CANM;EAgBbC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,IAAI,CAAC,KAAKR,UAAL,CAAgBS,EAArB,EAAyB;QACvB,OAAO,EAAP;MACF;;MACA,IAAIC,IAAG,GAAI,KAAKV,UAAL,CAAgBF,YAAhB,GAA+B,KAAKM,MAA/C;;MACA,IAAIM,IAAG,GAAI,CAAX,EAAc;QACZA,IAAG,GAAI,CAAP;MACF;;MACA,IAAID,EAAC,GAAIC,IAAG,GAAI,KAAKN,MAAL,GAAc,CAA9B;;MACA,IAAIK,EAAC,IAAK,KAAKT,UAAL,CAAgBW,SAA1B,EAAqC;QACnCF,EAAC,GAAI,KAAKT,UAAL,CAAgBW,SAArB;MACF;;MACA,IAAIC,UAAS,GAAI,EAAjB;;MACA,KAAK,IAAIC,IAAG,GAAIH,IAAhB,EAAsBG,IAAG,IAAKJ,EAA9B,EAAkCI,IAAI,EAAtC,EAA0C;QACxCD,UAAU,CAACE,IAAX,CAAgBD,IAAhB;MACF;;MACA,OAAOD,UAAP;IACD;;EAlBO,CAhBG;EAoCbG,OAAO,EAAE;IACPC,UAAU,CAACH,IAAD,EAAO;MACfI,OAAO,CAACC,GAAR,CAAY,KAAKlB,UAAL,CAAgBF,YAAhB,GAA+Be,IAA3C;MACA,KAAKM,KAAL,CAAW,UAAX;IACD;;EAJM;AApCI,CAAf","names":["data","current_page","props","pagination","type","Object","required","offset","Number","default","computed","p