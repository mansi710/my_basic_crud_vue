{"ast":null,"code":"import axios from \"axios\"; // import LaravelVuePagination from \"laravel-vue-pagination\";\n// import NewPagination from \"@/components/NewPagination.vue\";\n\nexport default {\n  name: \"ListOfBlog\",\n\n  // components: { NewPagination },\n  // components: { LaravelVuePagination },\n  data() {\n    return {\n      blogs: [],\n      pagination: {}\n    };\n  },\n\n  mounted() {\n    this.getBlogs();\n  },\n\n  methods: {\n    getBlogs(pagination) {\n      axios;\n      pagination = pagination || `/getAllBlog`;\n      fetch(pagination).then(response => {\n        // this.blogs = response.data;\n        this.blogs = response.data;\n        this.pagination = {\n          current_page: response.meta.current_page,\n          last_page: response.meta.last_page,\n          form_page: response.meta.from_page,\n          to_page: response.meta.to_page,\n          total_page: response.meta.total_page,\n          per_page: response.meta.per_page,\n          path_page: response.meta.path + \"?page\",\n          first_link: response.links.first_link,\n          last_link: response.links.last_link,\n          prev_link: response.links.prev_link,\n          next_link: response.links.next_link\n        };\n      }).catch(error => {\n        console.log(error);\n        console.log(\"handle server error from here\");\n        this.blogs = [];\n      });\n    },\n\n    async deleteBlog(id) {\n      if (confirm(\"Are you sure to delete this category ?\")) {\n        await axios.post(`destroyBlog/${id}`).then(response => {\n          this.getBlogs();\n\n          if (response) {\n            alert(\"blog deleted sucessfully\");\n          }\n        }).catch(error => {\n          console.log(error);\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6EA,OAAOA,KAAP,MAAkB,OAAlB,C,CACA;AACA;;AACA,eAAe;EACbC,IAAI,EAAE,YADO;;EAEb;EACA;EACAC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EADF;MAELC,UAAU,EAAE;IAFP,CAAP;EAID,CATY;;EAUbC,OAAO,GAAG;IACR,KAAKC,QAAL;EACD,CAZY;;EAabC,OAAO,EAAE;IACPD,QAAQ,CAACF,UAAD,EAAa;MACnBJ,KAAK;MACLI,UAAS,GAAIA,UAAS,IAAM,aAA5B;MACAI,KAAK,CAACJ,UAAD,CAAL,CACGK,IADH,CACSC,QAAD,IAAc;QAClB;QACA,KAAKP,KAAL,GAAaO,QAAQ,CAACR,IAAtB;QACA,KAAKE,UAAL,GAAkB;UAChBO,YAAY,EAAED,QAAQ,CAACE,IAAT,CAAcD,YADZ;UAEhBE,SAAS,EAAEH,QAAQ,CAACE,IAAT,CAAcC,SAFT;UAGhBC,SAAS,EAAEJ,QAAQ,CAACE,IAAT,CAAcG,SAHT;UAIhBC,OAAO,EAAEN,QAAQ,CAACE,IAAT,CAAcI,OAJP;UAKhBC,UAAU,EAAEP,QAAQ,CAACE,IAAT,CAAcK,UALV;UAMhBC,QAAQ,EAAER,QAAQ,CAACE,IAAT,CAAcM,QANR;UAOhBC,SAAS,EAAET,QAAQ,CAACE,IAAT,CAAcQ,IAAd,GAAqB,OAPhB;UAQhBC,UAAU,EAAEX,QAAQ,CAACY,KAAT,CAAeD,UARX;UAShBE,SAAS,EAAEb,QAAQ,CAACY,KAAT,CAAeC,SATV;UAUhBC,SAAS,EAAEd,QAAQ,CAACY,KAAT,CAAeE,SAVV;UAWhBC,SAAS,EAAEf,QAAQ,CAACY,KAAT,CAAeG;QAXV,CAAlB;MAaD,CAjBH,EAkBGC,KAlBH,CAkBUC,KAAD,IAAW;QAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;QACAC,OAAO,CAACC,GAAR,CAAY,+BAAZ;QACA,KAAK1B,KAAL,GAAa,EAAb;MACD,CAtBH;IAuBD,CA3BM;;IA4BP,MAAM2B,UAAN,CAAiBC,EAAjB,EAAqB;MACnB,IAAIC,OAAO,CAAC,wCAAD,CAAX,EAAuD;QACrD,MAAMhC,KAAI,CACPiC,IADG,CACG,eAAcF,EAAG,EADpB,EAEHtB,IAFG,CAEGC,QAAD,IAAc;UAClB,KAAKJ,QAAL;;UACA,IAAII,QAAJ,EAAc;YACZwB,KAAK,CAAC,0BAAD,CAAL;UACF;QACD,CAPG,EAQHR,KARG,CAQIC,KAAD,IAAW;UAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;QACD,CAVG,CAAN;MAWF;IACD;;EA1CM;AAbI,CAAf","names":["axios","name","data","blogs","pagination","mounted","getBlogs","methods","fetch","then","response","current_page","meta","last_page","form_page","from_page","to_page","total_page","per_page","path_page","path","first_link","links","last_link","prev_link","next_link","catch","error","console","log","deleteBlog","id","confirm","post","alert"],"sourceRoot":"","sources":["/var/www/html/newData/mansi/my-vuejs-final/src/views/ListOfBlog.vue"],"sourcesContent":["<template>\n  <link\n    href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css\"\n    rel=\"stylesheet\"\n    integrity=\"sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC\"\n    crossorigin=\"anonymous\"\n  />\n\n  <div class=\"row\">\n    <div class=\"col-12 mb-2 text-end\">\n      <router-link :to=\"{ name: 'apiAddNewBlog' }\" class=\"btn btn-primary\"\n        >Create</router-link\n      >\n    </div>\n    <div class=\"col-12\">\n      <div class=\"card\">\n        <div class=\"card-header\">\n          <h4>Blogs List</h4>\n        </div>\n        <div class=\"card-body\">\n          <div class=\"table-responsive\">\n            <table class=\"table table-bordered\">\n              <thead>\n                <tr>\n                  <th>ID</th>\n                  <th>Title</th>\n                  <th>Description</th>\n                  <th>Status</th>\n                  <th>Tag</th>\n                  <th>Order</th>\n                  <th>Actions</th>\n                </tr>\n              </thead>\n              <tbody v-if=\"blogs\">\n                <tr v-for=\"blog in blogs.data\" :key=\"blog.id\">\n                  <td>{{ blog.id }}</td>\n                  <td>{{ blog.title }}</td>\n                  <td>{{ blog.description }}</td>\n                  <td>{{ blog.status }}</td>\n                  <td>{{ blog.tag }}</td>\n                  <td>{{ blog.order }}</td>\n                  <td>\n                    <router-link\n                      :to=\"{ name: 'apiEditBlog', params: { id: blog.id } }\"\n                      class=\"btn btn-success\"\n                      >Edit</router-link\n                    >\n                    <button\n                      type=\"button\"\n                      @click=\"deleteBlog(blog.id)\"\n                      class=\"btn btn-danger\"\n                    >\n                      Delete\n                    </button>\n                  </td>\n                </tr>\n              </tbody>\n              <tbody v-else>\n                <tr>\n                  <td colspan=\"4\" align=\"center\">No Blogs Found.</td>\n                </tr>\n              </tbody>\n            </table>\n\n            <!-- <NewPagination\n              v-if=\"pagination.last_page > 1\"\n              :pagination=\"pagination\"\n              :offset=\"5\"\n              @paginate=\"getBlogs()\"\n            ></NewPagination> -->\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\n// import LaravelVuePagination from \"laravel-vue-pagination\";\n// import NewPagination from \"@/components/NewPagination.vue\";\nexport default {\n  name: \"ListOfBlog\",\n  // components: { NewPagination },\n  // components: { LaravelVuePagination },\n  data() {\n    return {\n      blogs: [],\n      pagination: {},\n    };\n  },\n  mounted() {\n    this.getBlogs();\n  },\n  methods: {\n    getBlogs(pagination) {\n      axios;\n      pagination = pagination || `/getAllBlog`;\n      fetch(pagination)\n        .then((response) => {\n          // this.blogs = response.data;\n          this.blogs = response.data;\n          this.pagination = {\n            current_page: response.meta.current_page,\n            last_page: response.meta.last_page,\n            form_page: response.meta.from_page,\n            to_page: response.meta.to_page,\n            total_page: response.meta.total_page,\n            per_page: response.meta.per_page,\n            path_page: response.meta.path + \"?page\",\n            first_link: response.links.first_link,\n            last_link: response.links.last_link,\n            prev_link: response.links.prev_link,\n            next_link: response.links.next_link,\n          };\n        })\n        .catch((error) => {\n          console.log(error);\n          console.log(\"handle server error from here\");\n          this.blogs = [];\n        });\n    },\n    async deleteBlog(id) {\n      if (confirm(\"Are you sure to delete this category ?\")) {\n        await axios\n          .post(`destroyBlog/${id}`)\n          .then((response) => {\n            this.getBlogs();\n            if (response) {\n              alert(\"blog deleted sucessfully\");\n            }\n          })\n          .catch((error) => {\n            console.log(error);\n          });\n      }\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}