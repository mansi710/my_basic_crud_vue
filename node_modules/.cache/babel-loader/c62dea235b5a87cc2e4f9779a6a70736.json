{"ast":null,"code":"export default {\n  props: {\n    pagination_prop: {\n      type: Number,\n      required: true\n    },\n    offset: {\n      type: Number,\n      default: 4\n    },\n\n    data() {\n      return {\n        pagination: this.pagination_prop\n      };\n    }\n\n  },\n  computed: {\n    pagesNumber() {\n      console.log(this.pagination.current_page);\n\n      if (!this.pagination.to) {\n        return [];\n      }\n\n      let from = this.pagination.current_page - this.offset;\n\n      if (from < 1) {\n        from = 1;\n      }\n\n      let to = from + this.offset * 2;\n\n      if (to >= this.pagination.last_page) {\n        to = this.pagination.last_page;\n      }\n\n      let pagesArray = [];\n\n      for (let page = from; page <= to; page++) {\n        pagesArray.push(page);\n      }\n\n      return pagesArray;\n    }\n\n  },\n  methods: {\n    da() {\n      console.log(this.pagination.current_page);\n    },\n\n    changePage(page) {\n      this.pagination.current_page = page;\n      this.$emit(\"paginate\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+CA,eAAe;EACbA,KAAK,EAAE;IACLC,eAAe,EAAE;MACfC,IAAI,EAAEC,MADS;MAEfC,QAAQ,EAAE;IAFK,CADZ;IAKLC,MAAM,EAAE;MACNH,IAAI,EAAEC,MADA;MAENG,OAAO,EAAE;IAFH,CALH;;IASLC,IAAI,GAAG;MACL,OAAO;QACLC,UAAU,EAAE,KAAKP;MADZ,CAAP;IAGD;;EAbI,CADM;EAgBbQ,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZC,OAAO,CAACC,GAAR,CAAY,KAAKJ,UAAL,CAAgBK,YAA5B;;MACA,IAAI,CAAC,KAAKL,UAAL,CAAgBM,EAArB,EAAyB;QACvB,OAAO,EAAP;MACF;;MACA,IAAIC,IAAG,GAAI,KAAKP,UAAL,CAAgBK,YAAhB,GAA+B,KAAKR,MAA/C;;MACA,IAAIU,IAAG,GAAI,CAAX,EAAc;QACZA,IAAG,GAAI,CAAP;MACF;;MACA,IAAID,EAAC,GAAIC,IAAG,GAAI,KAAKV,MAAL,GAAc,CAA9B;;MACA,IAAIS,EAAC,IAAK,KAAKN,UAAL,CAAgBQ,SAA1B,EAAqC;QACnCF,EAAC,GAAI,KAAKN,UAAL,CAAgBQ,SAArB;MACF;;MACA,IAAIC,UAAS,GAAI,EAAjB;;MACA,KAAK,IAAIC,IAAG,GAAIH,IAAhB,EAAsBG,IAAG,IAAKJ,EAA9B,EAAkCI,IAAI,EAAtC,EAA0C;QACxCD,UAAU,CAACE,IAAX,CAAgBD,IAAhB;MACF;;MACA,OAAOD,UAAP;IACD;;EAnBO,CAhBG;EAqCbG,OAAO,EAAE;IACPC,EAAE,GAAG;MACHV,OAAO,CAACC,GAAR,CAAY,K