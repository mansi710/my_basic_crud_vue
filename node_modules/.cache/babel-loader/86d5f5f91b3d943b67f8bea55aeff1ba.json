{"ast":null,"code":"import axios from \"axios\";\nimport ValidationError from \"@/components/ValidationError.vue\";\nexport default {\n  name: \"CreatePost\",\n\n  data() {\n    return {\n      components: {\n        ValidationError\n      },\n      validationErrors: \"\",\n      login: {\n        // userId: \"\",\n        email: \"\",\n        password: \"\"\n      }\n    };\n  },\n\n  methods: {\n    CreatePost() {\n      axios.post(\"login\", this.login).then(response => {\n        if (response.status == 200) {\n          localStorage.setItem(\"token\", response.data.access_token);\n          alert(\"sucess\");\n          this.$router.push(\"/home\");\n        } else {\n          alert(\"please enter email and password\");\n          console.log(this.validationErrors = response.data.error);\n          this.validationErrors = response.data.error;\n        }\n      }).catch(error => {\n        if (error.response.status == 401) {\n          alert(\"credentials are wrong\");\n          console.log(this.validationErrors = error.response.data.error);\n          this.validationErrors = error.response.data.error;\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsEA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,eAAP,MAA4B,kCAA5B;AACA,eAAe;EACbC,IAAI,EAAE,YADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,UAAU,EAAE;QAAEH;MAAF,CADP;MAELI,gBAAgB,EAAE,EAFb;MAGLC,KAAK,EAAE;QACL;QACAC,KAAK,EAAE,EAFF;QAGLC,QAAQ,EAAE;MAHL;IAHF,CAAP;EASD,CAZY;;EAcbC,OAAO,EAAE;IACPC,UAAU,GAAG;MACXV,KAAI,CACDW,IADH,CACQ,OADR,EACiB,KAAKL,KADtB,EAEGM,IAFH,CAESC,QAAD,IAAc;QAClB,IAAIA,QAAQ,CAACC,MAAT,IAAmB,GAAvB,EAA4B;UAC1BC,YAAY,CAACC,OAAb,CAAqB,OAArB,EAA8BH,QAAQ,CAACV,IAAT,CAAcc,YAA5C;UACAC,KAAK,CAAC,QAAD,CAAL;UACA,KAAKC,OAAL,CAAaC,IAAb,CAAkB,OAAlB;QACF,CAJA,MAIO;UACLF,KAAK,CAAC,iCAAD,CAAL;UACAG,OAAO,CAACC,GAAR,CAAa,KAAKjB,gBAAL,GAAwBQ,QAAQ,CAACV,IAAT,CAAcoB,KAAnD;UACA,KAAKlB,gBAAL,GAAwBQ,QAAQ,CAACV,IAAT,CAAcoB,KAAtC;QACF;MACD,CAZH,EAaGC,KAbH,CAaUD,KAAD,IAAW;QAChB,IAAIA,KAAK,CAACV,QAAN,CAAeC,MAAf,IAAyB,GAA7B,EAAkC;UAChCI,KAAK,CAAC,uBAAD,CAAL;UACAG,OAAO,CAACC,GAAR,CAAa,KAAKjB,gBAAL,GAAwBkB,KAAK,CAACV,QAAN,CAAeV,IAAf,CAAoBoB,KAAzD;UACA,KAAKlB,gBAAL,GAAwBkB,KAAK,CAACV,QAAN,CAAeV,IAAf,CAAoBoB,KAA5C;QACF;MACD,CAnBH;IAoBD;;EAtBM;AAdI,CAAf","names":["axios","ValidationError","name","data","components","validationErrors","login","email","password","methods","CreatePost","post","then","response","status","localStorage","setItem","access_token","alert","$router","push","console","log","error","catch"],"sourceRoot":"","sources":["/var/www/html/newData/mansi/my-vuejs-final/src/views/LoginC.vue"],"sourcesContent":["<template>\n  <link\n    href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css\"\n    rel=\"stylesheet\"\n    integrity=\"sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC\"\n    crossorigin=\"anonymous\"\n  />\n  <form @submit.prevent=\"CreatePost\">\n    <br />\n    <br />\n    <br />\n    <div class=\"card-header\">\n      <h4>Login</h4>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"title\" class=\"col-sm-2 col-form-label\"> Enter Email </label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"title\"\n          placeholder=\"enter your email\"\n          v-model=\"login.email\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"title\" class=\"col-sm-2 col-form-label\">Enter Password </label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"title\"\n          placeholder=\"enter your password\"\n          v-model=\"login.password\"\n        />\n      </div>\n    </div>\n    <!-- \n    <div class=\"form-group row\">\n      <div\n        class=\"col-sm-10 alert alert-danger\"\n        v-if=\"validationErrors\"\n        role=\"alert\"\n      >\n        {{ validationErrors }}\n      </div>\n    </div> -->\n    <validation-errors\n      :errors=\"validationErrors\"\n      v-if=\"validationErrors\"\n      class=\"col-sm-10 alert alert-danger\"\n      id=\"err\"\n    >\n      {{ validationErrors }}\n    </validation-errors>\n    <br />\n    <div class=\"form-group row\">\n      <div class=\"col-sm-10\">\n        <button type=\"submit\" class=\"btn btn-primary\">Login</button>\n      </div>\n    </div>\n    <br />\n    <router-link to=\"/apiForgotPassword\">Forgot Password</router-link>\n  </form>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport ValidationError from \"@/components/ValidationError.vue\";\nexport default {\n  name: \"CreatePost\",\n  data() {\n    return {\n      components: { ValidationError },\n      validationErrors: \"\",\n      login: {\n        // userId: \"\",\n        email: \"\",\n        password: \"\",\n      },\n    };\n  },\n\n  methods: {\n    CreatePost() {\n      axios\n        .post(\"login\", this.login)\n        .then((response) => {\n          if (response.status == 200) {\n            localStorage.setItem(\"token\", response.data.access_token);\n            alert(\"sucess\");\n            this.$router.push(\"/home\");\n          } else {\n            alert(\"please enter email and password\");\n            console.log((this.validationErrors = response.data.error));\n            this.validationErrors = response.data.error;\n          }\n        })\n        .catch((error) => {\n          if (error.response.status == 401) {\n            alert(\"credentials are wrong\");\n            console.log((this.validationErrors = error.response.data.error));\n            this.validationErrors = error.response.data.error;\n          }\n        });\n    },\n  },\n};\n</script>\n<style scoped>\n#err {\n  margin-top: 500px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}