{"ast":null,"code":"import axios from \"axios\";\nimport ValidationError from \"@/components/ValidationError.vue\";\nexport default {\n  name: \"CreatePost\",\n\n  data() {\n    return {\n      components: {\n        ValidationError\n      },\n      validationErrors: \"\",\n      login: {\n        // userId: \"\",\n        email: \"\",\n        password: \"\"\n      }\n    };\n  },\n\n  methods: {\n    CreatePost() {\n      axios.post(\"login\", this.login).then(response => {\n        if (response.status == 200) {\n          alert(\"sucess\");\n          localStorage.setItem(\"token\", response.data.access_token);\n          this.$router.push(\"/home\");\n        }\n      }).catch(error => {\n        if (error.response.status == 401) {\n          alert(\"credentials are wrong\");\n          console.log(this.validationErrors = error.response.data.error);\n          this.validationErrors = error.response.data.error;\n        }\n      });\n    }\n\n  },\n  components: {\n    ValidationError\n  }\n};","map":{"version":3,"mappings":"AA4DA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,eAAP,MAA4B,kCAA5B;AACA,eAAe;EACXC,IAAI,EAAE,YADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,UAAU,EAAE;QAAEH;MAAF,CADT;MAEHI,gBAAgB,EAAE,EAFf;MAGHC,KAAK,EAAE;QACH;QACAC,KAAK,EAAE,EAFJ;QAGHC,QAAQ,EAAE;MAHP;IAHJ,CAAP;EASH,CAZU;;EAaXC,OAAO,EAAE;IACLC,UAAU,GAAG;MACTV,KAAI,CACCW,IADL,CACU,OADV,EACmB,KAAKL,KADxB,EAEKM,IAFL,CAEWC,QAAD,IAAc;QACpB,IAAIA,QAAQ,CAACC,MAAT,IAAmB,GAAvB,EAA4B;UACxBC,KAAK,CAAC,QAAD,CAAL;UACAC,YAAY,CAACC,OAAb,CAAqB,OAArB,EAA8BJ,QAAQ,CAACV,IAAT,CAAce,YAA5C;UACA,KAAKC,OAAL,CAAaC,IAAb,CAAkB,OAAlB;QACJ;MACH,CARD,EASKC,KATL,CASYC,KAAD,IAAW;QAClB,IAAIA,KAAK,CAACT,QAAN,CAAeC,MAAf,IAAyB,GAA7B,EAAkC;UAC9BC,KAAK,CAAC,uBAAD,CAAL;UACAQ,OAAO,CAACC,GAAR,CAAa,KAAKnB,gBAAL,GAAwBiB,KAAK,CAACT,QAAN,CAAeV,IAAf,CAAoBmB,KAAzD;UACA,KAAKjB,gBAAL,GAAwBiB,KAAK,CAACT,QAAN,CAAeV,IAAf,CAAoBmB,KAA5C;QACJ;MACH,CAfD;IAgBH;;EAlBI,CAbE;EAiCXlB,UAAU,EAAE;IAAEH;EAAF;AAjCD,CAAf","names":["axios","ValidationError","name","data","components","validationErrors","login","email","password","methods","CreatePost","post","then","response","status","alert","localStorage","setItem","access_token","$router","push","catch","error","console","log"],"sourceRoot":"","sources":["/var/www/html/newData/mansi/my-vuejs-final/src/views/LoginC.vue"],"sourcesContent":["<template>\n  <link\n    href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css\"\n    rel=\"stylesheet\"\n    integrity=\"sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC\"\n    crossorigin=\"anonymous\"\n  />\n  <form @submit.prevent=\"CreatePost\">\n    <br />\n    <br />\n    <br />\n    <div class=\"card-header\">\n      <h4>Login</h4>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"title\" class=\"col-sm-2 col-form-label\"> Enter Email </label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"title\"\n          placeholder=\"enter your email\"\n          v-model=\"login.email\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"title\" class=\"col-sm-2 col-form-label\">Enter Password </label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"title\"\n          placeholder=\"enter your password\"\n          v-model=\"login.password\"\n        />\n      </div>\n    </div>\n    <validation-error\n      :errors=\"validationErrors\"\n      v-if=\"validationErrors\"\n      class=\"alert alert-danger\"\n      id=\"err\"\n    >\n      {{ validationErrors }}\n    </validation-error>\n    <br />\n    <div class=\"form-group row\">\n      <div class=\"col-sm-10\">\n        <button type=\"submit\" class=\"btn btn-primary\">Login</button>\n      </div>\n    </div>\n    <br />\n    <router-link to=\"/apiForgotPassword\">Forgot Password</router-link>\n  </form>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport ValidationError from \"@/components/ValidationError.vue\";\nexport default {\n    name: \"CreatePost\",\n    data() {\n        return {\n            components: { ValidationError },\n            validationErrors: \"\",\n            login: {\n                // userId: \"\",\n                email: \"\",\n                password: \"\",\n            },\n        };\n    },\n    methods: {\n        CreatePost() {\n            axios\n                .post(\"login\", this.login)\n                .then((response) => {\n                if (response.status == 200) {\n                    alert(\"sucess\");\n                    localStorage.setItem(\"token\", response.data.access_token);\n                    this.$router.push(\"/home\");\n                }\n            })\n                .catch((error) => {\n                if (error.response.status == 401) {\n                    alert(\"credentials are wrong\");\n                    console.log((this.validationErrors = error.response.data.error));\n                    this.validationErrors = error.response.data.error;\n                }\n            });\n        },\n    },\n    components: { ValidationError }\n};\n</script>\n<style scoped>\n#err {\n  margin-top: 500px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}