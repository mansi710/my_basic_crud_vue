{"ast":null,"code":"import axios from \"axios\";\nimport ValidationError from \"@/components/ValidationError.vue\";\nimport ErrorShow from \"@com\";\nexport default {\n  name: \"CreatePost\",\n\n  data() {\n    return {\n      components: {\n        ValidationError\n      },\n      validationErrors: \"\",\n      login: {\n        // userId: \"\",\n        email: \"\",\n        password: \"\"\n      }\n    };\n  },\n\n  methods: {\n    CreatePost() {\n      axios.post(\"login\", this.login).then(response => {\n        if (response.status == 200) {\n          localStorage.setItem(\"token\", response.data.access_token);\n          alert(\"sucess\");\n          this.$router.push(\"/home\");\n        }\n      }).catch(error => {\n        if (error.response.status == 401) {\n          alert(\"credentials are wrong\");\n          console.log(this.validationErrors = error.response.data.error);\n          this.validationErrors = error.response.data.error;\n        } else {\n          alert(\"please eter email and password\");\n          this.validationErrors = error.response.data.error;\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4DA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,eAAP,MAA4B,kCAA5B;AACA,OAAOC,SAAP,MAAsB,MAAtB;AACA,eAAe;EACbC,IAAI,EAAE,YADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,UAAU,EAAE;QAAEJ;MAAF,CADP;MAELK,gBAAgB,EAAE,EAFb;MAGLC,KAAK,EAAE;QACL;QACAC,KAAK,EAAE,EAFF;QAGLC,QAAQ,EAAE;MAHL;IAHF,CAAP;EASD,CAZY;;EAcbC,OAAO,EAAE;IACPC,UAAU,GAAG;MACXX,KAAI,CACDY,IADH,CACQ,OADR,EACiB,KAAKL,KADtB,EAEGM,IAFH,CAESC,QAAD,IAAc;QAClB,IAAIA,QAAQ,CAACC,MAAT,IAAmB,GAAvB,EAA4B;UAC1BC,YAAY,CAACC,OAAb,CAAqB,OAArB,EAA8BH,QAAQ,CAACV,IAAT,CAAcc,YAA5C;UACAC,KAAK,CAAC,QAAD,CAAL;UACA,KAAKC,OAAL,CAAaC,IAAb,CAAkB,OAAlB;QACF;MACD,CARH,EASGC,KATH,CASUC,KAAD,IAAW;QAChB,IAAIA,KAAK,CAACT,QAAN,CAAeC,MAAf,IAAyB,GAA7B,EAAkC;UAChCI,KAAK,CAAC,uBAAD,CAAL;UACAK,OAAO,CAACC,GAAR,CAAa,KAAKnB,gBAAL,GAAwBiB,KAAK,CAACT,QAAN,CAAeV,IAAf,CAAoBmB,KAAzD;UACA,KAAKjB,gBAAL,GAAwBiB,KAAK,CAACT,QAAN,CA