{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"AddNewBlog\",\n\n  data() {\n    return {\n      message: \"\",\n      blog: {\n        title: \"\",\n        description: \"\",\n        status: \"\",\n        tag: \"\",\n        order: \"\",\n        _method: \"patch\"\n      }\n    };\n  },\n\n  mounted() {\n    this.showBlog();\n  },\n\n  methods: {\n    async showBlog() {\n      await axios.get(`/api/category/${this.$route.params.id}`).then(response => {\n        const {\n          title,\n          description\n        } = response.data;\n        this.category.title = title;\n        this.category.description = description;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    async updateBlog() {\n      const response = await axios.post(\"/createNewBlog\", this.blog).then(this.$router.push({\n        name: \"apiListOfBlog\"\n      })).catch(error => console.log(error));\n\n      if (response) {\n        this.message = \"Blog Added Successfully\";\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0FA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,EAAE,YADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,EADJ;MAELC,IAAI,EAAE;QACJC,KAAK,EAAE,EADH;QAEJC,WAAW,EAAE,EAFT;QAGJC,MAAM,EAAE,EAHJ;QAIJC,GAAG,EAAE,EAJD;QAKJC,KAAK,EAAE,EALH;QAMJC,OAAO,EAAE;MANL;IAFD,CAAP;EAWD,CAdY;;EAebC,OAAO,GAAG;IACR,KAAKC,QAAL;EACD,CAjBY;;EAkBbC,OAAO,EAAE;IACP,MAAMD,QAAN,GAAiB;MACf,MAAMZ,KAAI,CACPc,GADG,CACE,iBAAgB,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAAG,EADxC,EAEHC,IAFG,CAEGC,QAAD,IAAc;QAClB,MAAM;UAAEd,KAAF;UAASC;QAAT,IAAyBa,QAAQ,CAACjB,IAAxC;QACA,KAAKkB,QAAL,CAAcf,KAAd,GAAsBA,KAAtB;QACA,KAAKe,QAAL,CAAcd,WAAd,GAA4BA,WAA5B;MACD,CANG,EAOHe,KAPG,CAOIC,KAAD,IAAW;QAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACD,CATG,CAAN;IAUD,CAZM;;IAaP,MAAMG,UAAN,GAAmB;MACjB,MAAMN,QAAO,GAAI,MAAMnB,KAAI,CACxB0B,IADoB,CACf,gBADe,EACG,KAAKtB,IADR,EAEpBc,IAFoB,CAEf,KAAKS,OAAL,CAAaC,IAAb,CAAkB;QAAE3B,IAAI,EAAE;MAAR,CAAlB,CAFe,EAGpBoB,KAHoB,CAGbC,KAAD,IAAWC,OAAO,CAACC,GA