  <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"title\"\n          placeholder=\"enter your title\"\n          v-model=\"blog.title\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"description\" class=\"col-sm-2 col-form-label\">\n        Enter Description\n      </label>\n      <div class=\"col-sm-8\">\n        <textarea\n          class=\"form-control\"\n          id=\"exampleFormControlTextarea1\"\n          rows=\"3\"\n          placeholder=\"enter description here\"\n          v-model=\"blog.description\"\n        ></textarea>\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"status\" class=\"col-sm-2 col-form-label\">Enter Status</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"status\"\n          placeholder=\"enter your status\"\n          v-model=\"blog.status\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"tag\" class=\"col-sm-2 col-form-label\">Enter Tag</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"tag\"\n          placeholder=\"enter your tag\"\n          v-model=\"blog.tag\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"order\" class=\"col-sm-2 col-form-label\">Enter order</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"number\"\n          class=\"form-control\"\n          id=\"order\"\n          placeholder=\"enter your order\"\n          v-model=\"blog.order\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <div class=\"col-sm-10\">\n        <button type=\"submit\" class=\"btn btn-primary\">Save</button>\n      </div>\n    </div>\n  </form>\n</template>\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"AddNewBlog\",\n  data() {\n    return {\n      message: \"\",\n      blog: {\n        title: \"\",\n        description: \"\",\n        status: \"\",\n        tag: \"\",\n        order: \"\",\n      },\n    };\n  },\n  methods: {\n    async createNewBlog() {\n           await axios\n        .post(\"/createNewBlog\", this.blog)\n        .then(this.$router.push({ name: \"apiListBlog\" }))\n        .catch((error) => console.log(error));\n      // if (response) {\n      //   // this.message = \"Blog Added Successfully\";\n      //   this.$router.push({ name: \"apiListOfBlog\" });\n      // }\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}