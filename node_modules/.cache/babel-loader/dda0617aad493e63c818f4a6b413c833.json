{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"EditBlog\",\n\n  data() {\n    return {\n      message: \"\",\n      blog: {\n        title: \"\",\n        description: \"\",\n        status: \"\",\n        tag: \"\",\n        order: \"\"\n      }\n    };\n  },\n\n  mounted() {\n    this.showBlog();\n  },\n\n  methods: {\n    async showBlog(id) {\n      await axios.get(`/editBlog/${this.$route.params.id}`).then(response => {\n        const {\n          title,\n          description,\n          status,\n          tag,\n          order\n        } = response.data;\n        this.blog.title = title;\n        this.blog.description = description;\n        this.blog.status = status;\n        this.blog.tag = tag;\n        this.blog.order = order;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    async updateBlog() {\n      await axios.put(`/updateBlog/${this.$route.params.id}`, this.blog).then(response => console.log(response)).catch(error => console.log(error));\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4FA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,EAAE,UADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,EADJ;MAELC,IAAI,EAAE;QACJC,KAAK,EAAE,EADH;QAEJC,WAAW,EAAE,EAFT;QAGJC,MAAM,EAAE,EAHJ;QAIJC,GAAG,EAAE,EAJD;QAKJC,KAAK,EAAE;MALH;IAFD,CAAP;EAUD,CAbY;;EAcbC,OAAO,GAAG;IACR,KAAKC,QAAL;EACD,CAhBY;;EAiBbC,OAAO,EAAE;IACP,MAAMD,QAAN,CAAeE,EAAf,EAAmB;MACjB,MAAMb,KAAI,CACPc,GADG,CACE,aAAY,KAAKC,MAAL,CAAYC,MAAZ,CAAmBH,EAAG,EADpC,EAEHI,IAFG,CAEGC,QAAD,IAAc;QAClB,MAAM;UAAEb,KAAF;UAASC,WAAT;UAAsBC,MAAtB;UAA8BC,GAA9B;UAAmCC;QAAnC,IAA6CS,QAAQ,CAAChB,IAA5D;QACA,KAAKE,IAAL,CAAUC,KAAV,GAAkBA,KAAlB;QACA,KAAKD,IAAL,CAAUE,WAAV,GAAwBA,WAAxB;QACA,KAAKF,IAAL,CAAUG,MAAV,GAAmBA,MAAnB;QACA,KAAKH,IAAL,CAAUI,GAAV,GAAgBA,GAAhB;QACA,KAAKJ,IAAL,CAAUK,KAAV,GAAkBA,KAAlB;MACD,CATG,EAUHU,KAVG,CAUIC,KAAD,IAAW;QAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACD,CAZG,CAAN;IAaD,CAfM;;IAgBP,MAAMG,UAAN,GAAmB;MACjB,MAAMvB,KAAI,CACPwB,GADG,CACE,eAAc,KAAKT,MAAL,CAAYC,MAAZ,CAAmBH,EAAG,EADtC,EACyC,KAAKT,IAD9C,EAEHa,IAFG,CAEGC,QAAD,IAAcG,OAAO,CAACC,GAAR,CAAYJ,QAAZ,CAFhB,EAGHC,KAHG,CAGIC,KAAD,IAAWC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CAHd,CAAN;IAID;;EArBM;AAjBI,CAAf","names":["axios","name","data","message","blog","title","description","status","tag","order","mounted","showBlog","methods","id","get","$route","params","then","response","catch","error","console","log","updateBlog","put"],"sourceRoot":"","sources":["/var/www/html/newData/mansi/my-vuejs-final/src/views/EditBlog.vue"],"sourcesContent":["<template>\n  <link\n    href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css\"\n    rel=\"stylesheet\"\n    integrity=\"sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC\"\n    crossorigin=\"anonymous\"\n  />\n\n  <form @submit.prevent=\"updateBlog\" id=\"page\">\n    <div class=\"card-header\">\n      <h4>Update Blog</h4>\n    </div>\n    <br />\n    <div>\n      {{ message }}\n    </div>\n\n    <div class=\"form-group row\">\n      <label for=\"title\" class=\"col-sm-2 col-form-label\">Enter Title</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"title\"\n          placeholder=\"enter your title\"\n          v-model=\"blog.title\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"description\" class=\"col-sm-2 col-form-label\">\n        Enter Description\n      </label>\n      <div class=\"col-sm-8\">\n        <textarea\n          class=\"form-control\"\n          id=\"exampleFormControlTextarea1\"\n          rows=\"3\"\n          placeholder=\"enter description here\"\n          v-model=\"blog.description\"\n        ></textarea>\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"status\" class=\"col-sm-2 col-form-label\">Enter Status</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"status\"\n          placeholder=\"enter your status\"\n          v-model=\"blog.status\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"tag\" class=\"col-sm-2 col-form-label\">Enter Tag</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"tag\"\n          placeholder=\"enter your tag\"\n          v-model=\"blog.tag\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"order\" class=\"col-sm-2 col-form-label\">Enter order</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"number\"\n          class=\"form-control\"\n          id=\"order\"\n          placeholder=\"enter your order\"\n          v-model=\"blog.order\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <div class=\"col-sm-10\">\n        <button type=\"submit\" class=\"btn btn-primary\">Save</button>\n      </div>\n    </div>\n  </form>\n</template>\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"EditBlog\",\n  data() {\n    return {\n      message: \"\",\n      blog: {\n        title: \"\",\n        description: \"\",\n        status: \"\",\n        tag: \"\",\n        order: \"\",\n      },\n    };\n  },\n  mounted() {\n    this.showBlog();\n  },\n  methods: {\n    async showBlog(id) {\n      await axios\n        .get(`/editBlog/${this.$route.params.id}`)\n        .then((response) => {\n          const { title, description, status, tag, order } = response.data;\n          this.blog.title = title;\n          this.blog.description = description;\n          this.blog.status = status;\n          this.blog.tag = tag;\n          this.blog.order = order;\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    async updateBlog() {\n      await axios\n        .put(`/updateBlog/${this.$route.params.id}`, this.blog)\n        .then((response) => console.log(response))\n        .catch((error) => console.log(error));\n    },\n  },\n};\n</script>\n<style>\n#page {\n  margin-top: 100px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}