{"ast":null,"code":"import axios from \"axios\";\nimport ValidationError from \"@/components/ValidationError.vue\";\nexport default {\n  name: \"AddNewBlog\",\n\n  data() {\n    return {\n      components: {\n        ValidationError\n      },\n      validationErrors: \"\",\n      blog: {\n        title: \"\",\n        description: \"\",\n        status: \"\",\n        tag: \"\",\n        order: \"\"\n      }\n    };\n  },\n\n  methods: {\n    async createNewBlog() {\n      const response = await axios.post(\"/createNewBlog\", this.blog).then(this.$router.push({\n        name: \"apiListOfBlog\"\n      })).catch(error => console.log(error));\n\n      if (response) {\n        this.message = \"Blog Added Successfully\";\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkGA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,eAAP,MAA4B,kCAA5B;AACA,eAAe;EACbC,IAAI,EAAE,YADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACDC,UAAU,EAAE;QAAEH;MAAF,CADX;MAELI,gBAAgB,EAAE,EAFb;MAGLC,IAAI,EAAE;QACJC,KAAK,EAAE,EADH;QAEJC,WAAW,EAAE,EAFT;QAGJC,MAAM,EAAE,EAHJ;QAIJC,GAAG,EAAE,EAJD;QAKJC,KAAK,EAAE;MALH;IAHD,CAAP;EAWD,CAdY;;EAebC,OAAO,EAAE;IACP,MAAMC,aAAN,GAAsB;MACpB,MAAMC,QAAO,GAAI,MAAMd,KAAI,CACxBe,IADoB,CACf,gBADe,EACG,KAAKT,IADR,EAEpBU,IAFoB,CAEf,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAAEhB,IAAI,EAAE;MAAR,CAAlB,CAFe,EAGpBiB,KAHoB,CAGbC,KAAD,IAAWC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CAHG,CAAvB;;MAIA,IAAIN,QAAJ,EAAc;QACZ,KAAKS,OAAL,GAAe,yBAAf;MACF;IACD;;EATM;AAfI,CAAf","names":["axios","ValidationError","name","data","components","validationErrors","blog","title","description","status","tag","order","methods","createNewBlog","response","post","then","$router","push","catch","error","console","log","message"],"sourceRoot":"","sources":["/var/www/html/newData/mansi/my-vuejs-final/src/views/AddNewBlog.vue"],"sourcesContent":["<template>\n  <link\n    href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css\"\n    rel=\"stylesheet\"\n    integrity=\"sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC\"\n    crossorigin=\"anonymous\"\n  />\n\n  <form @submit.prevent=\"createNewBlog\">\n    <div class=\"card-header\">\n      <h4>Add New Blog</h4>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <validation-errors\n        :errors=\"validationErrors\"\n        class=\"alert alert-danger\"\n        v-if=\"validationErrors\"\n        id=\"err\"\n      >\n        {{ validationErrors }}\n      </validation-errors>\n    </div>\n    <div class=\"form-group row\">\n      <label for=\"title\" class=\"col-sm-2 col-form-label\">Enter Title</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"title\"\n          placeholder=\"enter your title\"\n          v-model=\"blog.title\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"description\" class=\"col-sm-2 col-form-label\">\n        Enter Description\n      </label>\n      <div class=\"col-sm-8\">\n        <textarea\n          class=\"form-control\"\n          id=\"exampleFormControlTextarea1\"\n          rows=\"3\"\n          placeholder=\"enter description here\"\n          v-model=\"blog.description\"\n        ></textarea>\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"status\" class=\"col-sm-2 col-form-label\">Enter Status</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"status\"\n          placeholder=\"enter your status\"\n          v-model=\"blog.status\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"tag\" class=\"col-sm-2 col-form-label\">Enter Tag</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"tag\"\n          placeholder=\"enter your tag\"\n          v-model=\"blog.tag\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"order\" class=\"col-sm-2 col-form-label\">Enter order</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"number\"\n          class=\"form-control\"\n          id=\"order\"\n          placeholder=\"enter your order\"\n          v-model=\"blog.order\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <div class=\"col-sm-10\">\n        <button type=\"submit\" class=\"btn btn-primary\">Save</button>\n      </div>\n    </div>\n  </form>\n</template>\n<script>\nimport axios from \"axios\";\nimport ValidationError from \"@/components/ValidationError.vue\";\nexport default {\n  name: \"AddNewBlog\",\n  data() {\n    return {\n          components: { ValidationError },\n      validationErrors: \"\",\n      blog: {\n        title: \"\",\n        description: \"\",\n        status: \"\",\n        tag: \"\",\n        order: \"\",\n      },\n    };\n  },\n  methods: {\n    async createNewBlog() {\n      const response = await axios\n        .post(\"/createNewBlog\", this.blog)\n        .then(this.$router.push({ name: \"apiListOfBlog\" }))\n        .catch((error) => console.log(error));\n      if (response) {\n        this.message = \"Blog Added Successfully\";\n      }\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}