{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"CreatePost\",\n\n  data() {\n    return {\n      formData: {\n        // userId: \"\",\n        name: \"\",\n        email: \"\",\n        password: \"\",\n        confirm_password: \"\"\n      },\n      submitted: false\n    };\n  },\n\n  methods: {\n    CreatePost() {\n      axios.post(\"register\", this.formData).then(response => console.log(response)).catch(error => console.log(error));\n      this.$router.push({\n        name: \"apiLogin\"\n      });\n      this.submitted = true;\n      this.$v.touch();\n\n      if (this.$v.invalid) {\n        return false;\n      } else {\n        alert(\"Form Valid. Move to next screen\");\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkDA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,EAAE,YADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACR;QACAF,IAAI,EAAE,EAFE;QAGRG,KAAK,EAAE,EAHC;QAIRC,QAAQ,EAAE,EAJF;QAKRC,gBAAgB,EAAE;MALV,CADL;MAQLC,SAAS,EAAE;IARN,CAAP;EAUD,CAbY;;EAcbC,OAAO,EAAE;IACPC,UAAU,GAAG;MACXT,KAAI,CACDU,IADH,CACQ,UADR,EACoB,KAAKP,QADzB,EAEGQ,IAFH,CAESC,QAAD,IAAcC,OAAO,CAACC,GAAR,CAAYF,QAAZ,CAFtB,EAGGG,KAHH,CAGUC,KAAD,IAAWH,OAAO,CAACC,GAAR,CAAYE,KAAZ,CAHpB;MAKA,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAAEjB,IAAI,EAAE;MAAR,CAAlB;MACA,KAAKM,SAAL,GAAiB,IAAjB;MACA,KAAKY,EAAL,CAAQC,KAAR;;MACA,IAAI,KAAKD,EAAL,CAAQE,OAAZ,EAAqB;QACnB,OAAO,KAAP;MACF,CAFA,MAEO;QACLC,KAAK,CAAC,iCAAD,CAAL;MACF;IACD;;EAfM;AAdI,CAAf","names":["axios","name","data","formData","email","password","confirm_password","submitted","methods","CreatePost","post","then","response","console","log","catch","error","$router","push","$v","touch","invalid","alert"],"sourceRoot":"","sources":["/var/www/html/newData/mansi/my-vuejs-final/src/views/RegisterC.vue"],"sourcesContent":["<template>\n  <div>\n    <form @submit.prevent=\"CreatePost\">\n      <div>\n        <label for=\"name\">User Name</label>\n        <input\n          type=\"text\"\n          id=\"name\"\n          v-model=\"