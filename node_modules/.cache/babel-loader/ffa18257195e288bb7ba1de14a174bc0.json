{"ast":null,"code":"import axios from \"axios\";\nimport ValidationError from \"@/components/ValidationError.vue\";\nexport default {\n  name: \"CreatePost\",\n\n  data() {\n    return {\n      components: {\n        ValidationError\n      },\n      validationErrors: \"\",\n      login: {\n        // userId: \"\",\n        email: \"\",\n        password: \"\"\n      }\n    };\n  },\n\n  methods: {\n    CreatePost() {\n      axios.post(\"login\", this.login).then(response => {\n        localStorage.setItem(\"token\", response.data.access_token);\n\n        if (response.status == 200) {\n          alert(\"sucess\");\n          this.$router.push(\"/home\");\n        }\n      }).catch(error => {\n        if (error.response.status == 401) {\n          // alert(\"credentials are wrong\");\n          // console.log((this.validationErrors = error.response.data.error));\n          this.validationErrors = error.response.data.error;\n        }\n\n        if (error.response.status == 422) {\n          console.log(this.validationErrors = error.response.data.errors.password, this.validationErrors = error.response.data.errors.email); // this.validationErrors = error.response.data.emptyData;\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2DA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,eAAP,MAA4B,kCAA5B;AACA,eAAe;EACbC,IAAI,EAAE,YADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,UAAU,EAAE;QAAEH;MAAF,CADP;MAELI,gBAAgB,EAAE,EAFb;MAGLC,KAAK,EAAE;QACL;QACAC,KAAK,EAAE,EAFF;QAGLC,QAAQ,EAAE;MAHL;IAHF,CAAP;EASD,CAZY;;EAabC,OAAO,EAAE;IACPC,UAAU,GAAG;MACXV,KAAI,CACDW,IADH,CACQ,OADR,EACiB,KAAKL,KADtB,EAEGM,IAFH,CAESC,QAAD,IAAc;QAClBC,YAAY,CAACC,OAAb,CAAqB,OAArB,EAA8BF,QAAQ,CAACV,IAAT,CAAca,YAA5C;;QACA,IAAIH,QAAQ,CAACI,MAAT,IAAmB,GAAvB,EAA4B;UAC1BC,KAAK,CAAC,QAAD,CAAL;UACA,KAAKC,OAAL,CAAaC,IAAb,CAAkB,OAAlB;QACF;MACD,CARH,EASGC,KATH,CASUC,KAAD,IAAW;QAChB,IAAIA,KAAK,CAACT,QAAN,CAAeI,MAAf,IAAyB,GAA7B,EAAkC;UAChC;UACA;UACA,KAAKZ,gBAAL,GAAwBiB,KAAK,CAACT,QAAN,CAAeV,IAAf,CAAoBmB,KAA5C;QACF;;QACA,