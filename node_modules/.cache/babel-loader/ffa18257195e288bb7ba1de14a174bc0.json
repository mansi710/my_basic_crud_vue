rors }}\n    </validation-errors>\n    <br />\n    <div class=\"form-group row\">\n      <div class=\"col-sm-10\">\n        <button type=\"submit\" class=\"btn btn-primary\">Login</button>\n      </div>\n    </div>\n    <br />\n    <router-link to=\"/apiForgotPassword\">Forgot Password</router-link>\n  </form>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport ValidationError from \"@/components/ValidationError.vue\";\nexport default {\n  name: \"CreatePost\",\n  data() {\n    return {\n      components: { ValidationError },\n      validationErrors: \"\",\n      login: {\n        // userId: \"\",\n        email: \"\",\n        password: \"\",\n      },\n    };\n  },\n  methods: {\n    CreatePost() {\n      axios\n        .post(\"login\", this.login)\n        .then((response) => {\n          localStorage.setItem(\"token\", response.data.access_token);\n          if (response.status == 200) {\n            alert(\"sucess\");\n            this.$router.push(\"/home\");\n          }\n        })\n        .catch((error) => {\n          if (error.response.status == 401) {\n            // alert(\"credentials are wrong\");\n            // console.log((this.validationErrors = error.response.data.error));\n            this.validationErrors = error.response.data.error;\n          }\n          if (error.response.status == 422) {\n            console.log(\n              (this.validationErrors = error.response.data.errors.password),\n              (this.validationErrors = error.response.data.errors.email)\n            );\n            // this.validationErrors = error.response.data.emptyData;\n          }\n        });\n    },\n  },\n};\n</script>\n<style scoped>\n#err {\n  margin-top: 500px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}