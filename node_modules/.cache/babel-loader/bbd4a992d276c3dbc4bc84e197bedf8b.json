{"ast":null,"code":"export default {\n  props: {\n    pagination: {\n      type: Number,\n      required: true\n    },\n    offset: {\n      type: Number,\n      default: 4\n    }\n  },\n  computed: {\n    pagesNumber() {\n      if (!this.pagination.to) {\n        return [];\n      }\n\n      let from = this.pagination.current_page - this.offset;\n\n      if (from < 1) {\n        from = 1;\n      }\n\n      let to = from + this.offset * 2;\n\n      if (to >= this.pagination.last_page) {\n        to = this.pagination.last_page;\n      }\n\n      let pagesArray = [];\n\n      for (let page = from; page <= to; page++) {\n        pagesArray.push(page);\n      }\n\n      return pagesArray;\n    }\n\n  },\n  methods: {\n    da() {\n      console.log(this.pagination);\n    } // changePage(page) {\n    //   console.log(page);\n    //   this.pagination.current_page = page;\n    //   this.$emit(\"paginate\");\n    // },\n\n\n  }\n};","map":{"version":3,"mappings":"AAgCA,eAAe;EACbA,KAAK,EAAE;IACLC,UAAU,EAAE;MACVC,IAAI,EAAEC,MADI;MAEVC,QAAQ,EAAE;IAFA,CADP;IAKLC,MAAM,EAAE;MACNH,IAAI,EAAEC,MADA;MAENG,OAAO,EAAE;IAFH;EALH,CADM;EAWbC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,IAAI,CAAC,KAAKP,UAAL,CAAgBQ,EAArB,EAAyB;QACvB,OAAO,EAAP;MACF;;MACA,IAAIC,IAAG,GAAI,KAAKT,UAAL,CAAgBU,YAAhB,GAA+B,KAAKN,MAA/C;;MACA,IAAIK,IAAG,GAAI,CAAX,EAAc;QACZA,IAAG,GAAI,CAAP;MACF;;MACA,IAAID,EAAC,GAAIC,IAAG,GAAI,KAAKL,MAAL,GAAc,CAA9B;;MACA,IAAII,EAAC,IAAK,KAAKR,UAAL,CAAgBW,SAA1B,EAAqC;QACnCH,EAAC,GAAI,KAAKR,UAAL,CAAgBW,SAArB;MACF;;MACA,IAAIC,UAAS,GAAI,EAAjB;;MACA,KAAK,IAAIC,IAAG,GAAIJ,IAAhB,EAAsBI,IAAG,IAAKL,EAA9B,EAAkCK,IAAI,EAAtC,EAA0C;QACxCD,UAAU,CAACE,IAAX,CAAgBD,IAAhB;MACF;;MACA,OAAOD,UAAP;IACD;;EAlBO,CAXG;EA+BbG,OAAO,EAAE;IACPC,EAAE,GAAG;MACHC,OAAO,CAACC,GAAR,CAAY,KAAKlB,UAAjB;IACD,CAHM,CAKP;IACA;IACA;IACA;IACA;;;EATO;AA/BI,CAAf","names":["props","pagination","type","Number","required","offset","default","computed","pagesNumber","to","from","current_page","last_page","pagesArray","page","push","methods","da","con