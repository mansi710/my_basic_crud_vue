\n        // userId: \"\",\n        email: \"\",\n        password: \"\",\n      },\n    };\n  },\n\n  methods: {\n    CreatePost() {\n      axios\n        .post(\"login\", this.data)\n        .then((response) => {\n          if (response.status == 200) {\n            alert(\"sucess\");\n            this.$router.push(\"/home\");\n          }\n        })\n        .catch((error) => {\n          if (error.response.status == 401) {\n            alert(\"credentials are wrong\");\n            this.validationErrors = error.response.data.errors;\n          }\n        });\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}