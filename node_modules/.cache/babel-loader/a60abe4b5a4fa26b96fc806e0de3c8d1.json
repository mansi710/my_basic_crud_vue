{"ast":null,"code":"import axios from \"axios\";\nimport ValidationError from \"@/components/ValidationError.vue\";\nexport default {\n  name: \"CreatePost\",\n\n  data() {\n    return {\n      components: {\n        ValidationError\n      },\n      ValidationError: \"\",\n      data: {\n        // userId: \"\",\n        email: \"\",\n        password: \"\"\n      }\n    };\n  },\n\n  methods: {\n    CreatePost() {\n      axios.post(\"login\", this.data).then(response => {\n        if (response.status == 200) {\n          alert(\"sucess\");\n          this.$router.push(\"/home\");\n        }\n      }).catch(error => {\n        if (error.response.status == 401) {\n          alert(\"credentials are wrong\");\n          this.validationErrors = error.response.data.errors;\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmEA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,eAAP,MAA4B,kCAA5B;AACA,eAAe;EACbC,IAAI,EAAE,YADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,UAAU,EAAE;QAAEH;MAAF,CADP;MAELA,eAAe,EAAE,EAFZ;MAGLE,IAAI,EAAE;QACJ;QACAE,KAAK,EAAE,EAFH;QAGJC,QAAQ,EAAE;MAHN;IAHD,CAAP;EASD,CAZY;;EAcbC,OAAO,EAAE;IACPC,UAAU,GAAG;MACXR,KAAI,CACDS,IADH,CACQ,OADR,EACiB,KAAKN,IADtB,EAEGO,IAFH,CAESC,QAAD,IAAc;QAClB,IAAIA,QAAQ,CAACC,MAAT,IAAmB,GAAvB,EAA4B;UAC1BC,KAAK,CAAC,QAAD,CAAL;UACA,KAAKC,OAAL,CAAaC,IAAb,CAAkB,OAAlB;QACF;MACD,CAPH,EAQGC,KARH,CAQUC,KAAD,IAAW;QAChB,IAAIA,KAAK,CAACN,QAAN,CAAeC,MAAf,IAAyB,GAA7B,EAAkC;UAChCC,KAAK,CAAC,uBAAD,CAAL;UACA,KAAKK,gBAAL,GAAwBD,KAAK,CAACN,QAAN,CAAeR,IAAf,CAAoBgB,MAA5C;QACF;MACD,CAbH;IAcD;;EAhBM;AAdI,CAAf","names":["axios","ValidationError","name","data","components","email","password","methods","CreatePost","post","then","response","status","alert","$router","push","catch","error","validationErrors","errors"],"sourceRoot":"","sources":["/var/www/html/newData/mansi/my-vuejs-final/src/views/LoginC.vue"],"sourcesContent":["<template>\n  <link\n    href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css\"\n    rel=\"stylesh