{"ast":null,"code":"import axios from \"axios\"; // import LaravelVuePagination from \"laravel-vue-pagination\";\n\nimport PaginationComponent from \"@/components/PaginationComponent.vue\";\nexport default {\n  name: \"ListOfBlog\",\n\n  // components: { PaginationComponent },\n  // components: { LaravelVuePagination },\n  data() {\n    return {\n      blogs: {} // current_page: 1,\n      // total: 10,\n\n    };\n  },\n\n  mounted() {\n    this.getBlogs();\n  },\n\n  methods: {\n    async getBlogs(page = 1) {\n      await axios.get(\"/getAllBlog?page=\" + page).then(response => {\n        this.blogs = response.data;\n      }).catch(error => {\n        console.log(error);\n        console.log(\"handle server error from here\");\n        this.blogs = [];\n      });\n    },\n\n    async deleteBlog(id) {\n      if (confirm(\"Are you sure to delete this category ?\")) {\n        await axios.post(`destroyBlog/${id}`).then(response => {\n          this.getBlogs();\n\n          if (response) {\n            alert(\"blog deleted sucessfully\");\n          }\n        }).catch(error => {\n          console.log(error);\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2EA,OAAOA,KAAP,MAAkB,OAAlB,C,CACA;;AACA,OAAOC,mBAAP,MAAgC,sCAAhC;AACA,eAAe;EACbC,IAAI,EAAE,YADO;;EAEb;EACA;EACAC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EADF,CAEL;MACA;;IAHK,CAAP;EAKD,CAVY;;EAWbC,OAAO,GAAG;IACR,KAAKC,QAAL;EACD,CAbY;;EAcbC,OAAO,EAAE;IACP,MAAMD,QAAN,CAAeE,IAAG,GAAI,CAAtB,EAAyB;MACvB,MAAMR,KAAI,CACPS,GADG,CACC,sBAAsBD,IADvB,EAEHE,IAFG,CAEGC,QAAD,IAAc;QAClB,KAAKP,KAAL,GAAaO,QAAQ,CAACR,IAAtB;MACD,CAJG,EAKHS,KALG,CAKIC,KAAD,IAAW;QAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;QACAC,OAAO,CAACC,GAAR,CAAY,+BAAZ;QACA,KAAKX,KAAL,GAAa,EAAb;MACD,CATG,CAAN;IAUD,CAZM;;IAaP,MAAMY,UAAN,CAAiBC,EAAjB,EAAqB;MACnB,IAAIC,OAAO,CAAC,wCAAD,CAAX,EAAuD;QACrD,MAAMlB,KAAI,CACPmB,IADG,CACG,eAAcF,EAAG,EADpB,EAEHP,IAFG,CAEGC,QAAD,IAAc;UAClB,KAAKL,QAAL;;UACA,IAAIK,QAAJ,EAAc;YACZS,KAAK,CAAC,0BAAD,CAAL;UACF;QACD,CAPG,EAQHR,KAR