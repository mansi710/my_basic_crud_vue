{"ast":null,"code":"import axios from \"axios\";\nimport ValidationError from \"@/components/ValidationError.vue\";\nexport default {\n  name: \"AddNewBlog\",\n\n  data() {\n    return {\n      components: {\n        ValidationError\n      },\n      validationErrors: \"\",\n      blog: {\n        title: \"\",\n        description: \"\",\n        status: \"\",\n        tag: \"\",\n        order: \"\"\n      },\n      notifmsg: \"\"\n    };\n  },\n\n  methods: {\n    createNewBlog() {\n      axios.post(\"/createNewBlog\", this.blog, this.allerros = []).then(response => {\n        if (response.status == 200) {\n          this.$router.push({\n            name: \"apiListOfBlog\"\n          });\n        }\n      }).catch(e => {\n        if (e.response.status == 422) {\n          this.notifmsg = e.response.data;\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2GA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,eAAP,MAA4B,kCAA5B;AACA,eAAe;EACbC,IAAI,EAAE,YADO;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,UAAU,EAAE;QAAEH;MAAF,CADP;MAELI,gBAAgB,EAAE,EAFb;MAGLC,IAAI,EAAE;QACJC,KAAK,EAAE,EADH;QAEJC,WAAW,EAAE,EAFT;QAGJC,MAAM,EAAE,EAHJ;QAIJC,GAAG,EAAE,EAJD;QAKJC,KAAK,EAAE;MALH,CAHD;MAULC,QAAQ,EAAE;IAVL,CAAP;EAYD,CAhBY;;EAiBbC,OAAO,EAAE;IACPC,aAAa,GAAG;MACdd,KAAI,CACDe,IADH,CACQ,gBADR,EAC0B,KAAKT,IAD/B,EACsC,KAAKU,QAAL,GAAgB,EADtD,EAEGC,IAFH,CAESC,QAAD,IAAc;QAClB,IAAIA,QAAQ,CAACT,MAAT,IAAmB,GAAvB,EAA4B;UAC1B,KAAKU,OAAL,CAAaC,IAAb,CAAkB;YAAElB,IAAI,EAAE;UAAR,CAAlB;QACF;MACD,CANH,EAOGmB,KAPH,CAOUC,CAAD,IAAO;QACZ,IAAIA,CAAC,CAACJ,QAAF,CAAWT,MAAX,IAAqB,GAAzB,EAA8B;UAC5B,KAAKG,QAAL,GAAgBU,CAAC,CAACJ,QAAF,CAAWf,IAA3B;QACF;MACD,CAXH;IAYD;;EAdM;AAjBI,CAAf","names":["axios","ValidationError","name","data","components","validationErrors","blog","title","description","status","tag","order","notifmsg","methods","createNewBlog","post","allerros","then","response","$router","push","catch","e"],"sourceRoot":"","sources":["/var/www/html/newData/mansi/my-vuejs-final/src/views/AddNewBlog.vue"],"sourcesContent":["<template>\n  <link\n    href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css\"\n    rel=\"stylesheet\"\n    integrity=\"sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC\"\n    crossorigin=\"anonymous\"\n  />\n\n  <form @submit.prevent=\"createNewBlog\">\n    <div class=\"card-header\">\n      <h4>Add New Blog</h4>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <validation-errors\n        :errors=\"validationErrors\"\n        class=\"alert alert-danger\"\n        v-if=\"validationErrors\"\n        id=\"err\"\n      >\n        {{ validationErrors }}\n      </validation-errors>\n      <!-- <div v-for=\"(errorArray, idx) in notifmsg\" :key=\"idx\">\n        <ul\n          class=\"alert alert-danger\"\n          v-for=\"(allErrors, idx) in errorArray\"\n          :key=\"idx\"\n        >\n          <li class=\"error\">{{ allErrors }}</li>\n        </ul>\n      </div> -->\n    </div>\n    <div class=\"form-group row\">\n      <label for=\"title\" class=\"col-sm-2 col-form-label\">Enter Title</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"title\"\n          placeholder=\"enter your title\"\n          v-model=\"blog.title\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"description\" class=\"col-sm-2 col-form-label\">\n        Enter Description\n      </label>\n      <div class=\"col-sm-8\">\n        <textarea\n          class=\"form-control\"\n          id=\"exampleFormControlTextarea1\"\n          rows=\"3\"\n          placeholder=\"enter description here\"\n          v-model=\"blog.description\"\n        ></textarea>\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"status\" class=\"col-sm-2 col-form-label\">Enter Status</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"status\"\n          placeholder=\"enter your status\"\n          v-model=\"blog.status\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"tag\" class=\"col-sm-2 col-form-label\">Enter Tag</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"tag\"\n          placeholder=\"enter your tag\"\n          v-model=\"blog.tag\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <label for=\"order\" class=\"col-sm-2 col-form-label\">Enter order</label>\n      <div class=\"col-sm-8\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"order\"\n          placeholder=\"enter your order\"\n          v-model=\"blog.order\"\n        />\n      </div>\n    </div>\n    <br />\n    <div class=\"form-group row\">\n      <div class=\"col-sm-10\">\n        <button type=\"submit\" class=\"btn btn-primary\">Save</button>\n      </div>\n    </div>\n  </form>\n</template>\n<script>\nimport axios from \"axios\";\nimport ValidationError from \"@/components/ValidationError.vue\";\nexport default {\n  name: \"AddNewBlog\",\n\n  data() {\n    return {\n      components: { ValidationError },\n      validationErrors: \"\",\n      blog: {\n        title: \"\",\n        description: \"\",\n        status: \"\",\n        tag: \"\",\n        order: \"\",\n      },\n      notifmsg: \"\",\n    };\n  },\n  methods: {\n    createNewBlog() {\n      axios\n        .post(\"/createNewBlog\", this.blog, (this.allerros = []))\n        .then((response) => {\n          if (response.status == 200) {\n            this.$router.push({ name: \"apiListOfBlog\" });\n          }\n        })\n        .catch((e) => {\n          if (e.response.status == 422) {\n            this.notifmsg = e.response.data;\n          }\n        });\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}