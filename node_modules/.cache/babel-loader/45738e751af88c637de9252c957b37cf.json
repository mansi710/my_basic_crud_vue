{"ast":null,"code":"import axios from \"axios\"; // import LaravelVuePagination from \"laravel-vue-pagination\";\n// import NewPagination from \"@/components/NewPagination.vue\";\n\nexport default {\n  name: \"ListOfBlog\",\n\n  // components: { NewPagination },\n  // components: { LaravelVuePagination },\n  data() {\n    return {\n      blogs: [],\n      pagination: {}\n    };\n  },\n\n  mounted() {\n    this.getBlogs();\n  },\n\n  methods: {\n    getBlogs(pagination) {\n      axios;\n      pagination = pagination || `/getAllBlog`;\n      fetch(pagination).then(response => {\n        // this.blogs = response.data;\n        this.blogs = response.data;\n        this.pagination = {\n          current_page: response.current_page,\n          last_page: response.meta.last_page,\n          form_page: response.meta.from_page,\n          to_page: response.meta.to_page,\n          total_page: response.meta.total_page,\n          per_page: response.meta.per_page,\n          path_page: response.meta.path + \"?page\",\n          first_page_url: response.links.first_page_url,\n          last_page_url: response.links.last_page_url,\n          prev__page_url: response.links.prev__page_url,\n          next_page_url: response.links.next_page_url\n        };\n      }).catch(error => {\n        console.log(error);\n        console.log(\"handle server error from here\");\n        this.blogs = [];\n      });\n    },\n\n    async deleteBlog(id) {\n      if (confirm(\"Are you sure to delete this category ?\")) {\n        await axios.post(`destroyBlog/${id}`).then(response => {\n          this.getBlogs();\n\n          if (response) {\n            alert(\"blog deleted sucessfully\");\n          }\n        }).catch(error => {\n          console.log(error);\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+IA,OAAOA,KAAP,MAAkB,OAAlB,C,CACA;AACA;;AACA,eAAe;EACbC,IAAI,EAAE,YADO;;EAEb;EACA;EACAC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EADF;MAELC,UAAU,EAAE;IAFP,CAAP;EAID,CATY;;EAUbC,OAAO,GAAG;IACR,KAAKC,QAAL;EACD,CAZY;;EAabC,OAAO,EAAE;IACPD,QAAQ,CAACF,UAAD,EAAa;MACnBJ,KAAK;MACLI,UAAS,GAAIA,UAAS,IAAM,aAA5B;MACAI,KAAK,CAACJ,UAAD,CAAL,CACGK,IADH,CACSC,QAAD,IAAc;QAClB;QACA,KAAKP,KAAL,GAAaO,QAAQ,CAACR,IAAtB;QACA,KAAKE,UAAL,GAAkB;UAChBO,YAAY,EAAED,QAAQ,CAACC,YADP;UAEhBC,SAAS,EAAEF,QAAQ,CAACG,IAAT,CAAcD,SAFT;UAGhBE,SAAS,EAAEJ,QAAQ,CAACG,IAAT,CAAcE,SAHT;UAIhBC,OAAO,EAAEN,QAAQ,CAACG,IAAT,CAAcG,OAJP;UAKhBC,UAAU,EAAEP,QAAQ,CAACG,IAAT,CAAcI,UALV;UAMhBC,QAAQ,EAAER,QAAQ,CAACG,IAAT,CAAcK,QANR;UAOhBC,SAAS,EAAET,QAAQ,CAACG,IAAT,CAAcO,IAAd,GAAqB,OAPhB;UAQhBC,cAAc,EAAEX,QAAQ,CAACY,KAAT,CAAeD,cARf;UAShBE,aAAa,EAAEb,QAAQ,CAACY,KAAT,CAAeC,aATd;UAUhBC,cAAc,EAAEd,QAAQ,CAACY,KAAT,CAAeE,cAVf;UAWhBC,aAAa,EAAEf,QAAQ,CAACY,KAAT,CAAeG;QAXd,CAAlB;MAaD,CAjBH,EAkBGC,KAlBH,CAkBUC,KAAD,IAAW;QAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;QACAC,OAAO,CAACC,GAAR,CAAY,+BAAZ;QACA,KAAK1B,KAAL,GAAa,EAAb;MACD,CAtBH;IAuBD,CA3BM;;IA4BP,MAAM2B,UAAN,CAAiBC,EAAjB,EAAqB;MACnB,IAAIC,OAAO,CAAC,wCAAD,CAAX,EAAuD;QACrD,MAAMhC,KAAI,CACPiC,IADG,CACG,eAAcF,EAAG,EADpB,EAEHtB,IAFG,CAEGC,QAAD,IAAc;UAClB,KAAKJ,QAAL;;UACA,IAAII,QAAJ,EAAc;YACZwB,KAAK,CAAC,0BAAD,CAAL;UACF;QACD,CAPG,EAQHR,KARG,CAQIC,KAAD,IAAW;UAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;QACD,CAVG,CAAN;MAWF;IACD;;EA1CM;AAbI,CAAf","names":["axios","name","data","blogs","pagination","mounted","getBlogs","methods","fetch","then","response","current_page","last_page","meta","form_page","from_page","to_page","total_page","per_page","path_page","path","first_page_url","links","last_page_url","prev__page_url","next_page_url","catch","error","console","log","deleteBlog","id","confirm","post","alert"],"sourceRoot":"","sources":["/var/www/html/newData/mansi/my-vuejs-final/src/views/ListOfBlog.vue"],"sourcesContent":["<template>\n  <link\n    href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css\"\n    rel=\"stylesheet\"\n    integrity=\"sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC\"\n    crossorigin=\"anonymous\"\n  />\n\n  <div class=\"row\">\n    <div class=\"col-12 mb-2 text-end\">\n      <router-link :to=\"{ name: 'apiAddNewBlog' }\" class=\"btn btn-primary\"\n        >Create</router-link\n      >\n    </div>\n    <div class=\"col-12\">\n      <div class=\"card\">\n        <div class=\"card-header\">\n          <h4>Blogs List</h4>\n        </div>\n        <div class=\"card-body\">\n          <div class=\"table-responsive\">\n            <table class=\"table table-bordered\">\n              <thead>\n                <tr>\n                  <th>ID</th>\n                  <th>Title</th>\n                  <th>Description</th>\n                  <th>Status</th>\n                  <th>Tag</th>\n                  <th>Order</th>\n                  <th>Actions</th>\n                </tr>\n              </thead>\n              <tbody v-if=\"blogs\">\n                <tr v-for=\"blog in blogs.data\" :key=\"blog.id\">\n                  <td>{{ blog.id }}</td>\n                  <td>{{ blog.title }}</td>\n                  <td>{{ blog.description }}</td>\n                  <td>{{ blog.status }}</td>\n                  <td>{{ blog.tag }}</td>\n                  <td>{{ blog.order }}</td>\n                  <td>\n                    <router-link\n                      :to=\"{ name: 'apiEditBlog', params: { id: blog.id } }\"\n                      class=\"btn btn-success\"\n                      >Edit</router-link\n                    >\n                    <button\n                      type=\"button\"\n                      @click=\"deleteBlog(blog.id)\"\n                      class=\"btn btn-danger\"\n                    >\n                      Delete\n                    </button>\n                  </td>\n                </tr>\n              </tbody>\n              <tbody v-else>\n                <tr>\n                  <td colspan=\"4\" align=\"center\">No Blogs Found.</td>\n                </tr>\n              </tbody>\n            </table>\n\n            <!-- <NewPagination\n              v-if=\"pagination.last_page > 1\"\n              :pagination=\"pagination\"\n              :offset=\"5\"\n              @paginate=\"getBlogs()\"\n            ></NewPagination> -->\n            <div class=\"row\">\n              <div class=\"col-md-8\">\n                <nav>\n                  <ul class=\"pagination\">\n                    <li\n                      v-bind:class=\"{ disabled: !pagination.first_page_url }\"\n                      class=\"page-item\"\n                    >\n                      <a\n                        href=\"#\"\n                        class=\"page-link\"\n                        @click=\"getBlogs(pagination.first_page_url)\"\n                        >First</a\n                      >\n                    </li>\n\n                    <li\n                      v-bind:class=\"{ disabled: !pagination.first_page_url }\"\n                      class=\"page-item\"\n                    >\n                      <a\n                        href=\"#\"\n                        class=\"page-link\"\n                        @click=\"getBlogs(pagination.first_page_url)\"\n                      ></a>\n                    </li>\n                    <li\n                      v-bind:class=\"{ disabled: !pagination.first_page_url }\"\n                      class=\"page-item\"\n                    >\n                      <a\n                        href=\"#\"\n                        class=\"page-link\"\n                        @click=\"getBlogs(pagination.first_page_url)\"\n                      ></a>\n                    </li>\n                    <li\n                      v-bind:class=\"{ disabled: !pagination.first_page_url }\"\n                      class=\"page-item\"\n                    >\n                      <a\n                        href=\"#\"\n                        class=\"page-link\"\n                        @click=\"getBlogs(pagination.first_page_url)\"\n                      ></a>\n                    </li>\n                    <li\n                      v-bind:class=\"{ disabled: !pagination.first_page_url }\"\n                      class=\"page-item\"\n                    >\n                      <a\n                        href=\"#\"\n                        class=\"page-link\"\n                        @click=\"getBlogs(pagination.first_page_url)\"\n                      ></a>\n                    </li>\n                  </ul>\n                </nav>\n              </div>\n              <div class=\"col-md-4\">\n                Page:\n                {{ pagination.form_page }} - {{ pagination.to_page }} Total:{{\n                  pagination.total_page\n                }}\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\n// import LaravelVuePagination from \"laravel-vue-pagination\";\n// import NewPagination from \"@/components/NewPagination.vue\";\nexport default {\n  name: \"ListOfBlog\",\n  // components: { NewPagination },\n  // components: { LaravelVuePagination },\n  data() {\n    return {\n      blogs: [],\n      pagination: {},\n    };\n  },\n  mounted() {\n    this.getBlogs();\n  },\n  methods: {\n    getBlogs(pagination) {\n      axios;\n      pagination = pagination || `/getAllBlog`;\n      fetch(pagination)\n        .then((response) => {\n          // this.blogs = response.data;\n          this.blogs = response.data;\n          this.pagination = {\n            current_page: response.current_page,\n            last_page: response.meta.last_page,\n            form_page: response.meta.from_page,\n            to_page: response.meta.to_page,\n            total_page: response.meta.total_page,\n            per_page: response.meta.per_page,\n            path_page: response.meta.path + \"?page\",\n            first_page_url: response.links.first_page_url,\n            last_page_url: response.links.last_page_url,\n            prev__page_url: response.links.prev__page_url,\n            next_page_url: response.links.next_page_url,\n          };\n        })\n        .catch((error) => {\n          console.log(error);\n          console.log(\"handle server error from here\");\n          this.blogs = [];\n        });\n    },\n    async deleteBlog(id) {\n      if (confirm(\"Are you sure to delete this category ?\")) {\n        await axios\n          .post(`destroyBlog/${id}`)\n          .then((response) => {\n            this.getBlogs();\n            if (response) {\n              alert(\"blog deleted sucessfully\");\n            }\n          })\n          .catch((error) => {\n            console.log(error);\n          });\n      }\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}